  "keywords": [
    "EU Energy Label",
    "Energy Label",
    "AngularJS",
    "Angular",
    "SASS"
  ],
  "author": "Gabriele Destefanis <gabriele.destefanis@gmail.com> (http://destefanis.eu/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/destegabry/angular-energy-label/issues"
  },
  "homepage": "https://github.com/destegabry/angular-energy-label",
  "dependencies": {
    "angular": "^1.5.0"
  },
  "peerDependencies": {
    "bourbon": "^4.3.4"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "bourbon": "^4.3.4",
    "browser-sync": "^2.18.13",
    "gulp": "gulpjs/gulp.git#4.0",
    "gulp-angular-embed-templates": "^2.3.0",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-babel": "^7.0.0",
    "gulp-clean": "^0.3.2",
    "gulp-clean-css": "^3.9.0",
    "gulp-concat": "^2.6.1",
    "gulp-header": "^1.8.9",
    "gulp-iife": "^0.3.0",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.6.1",
    "gulp-uglify": "^3.0.0",
    "xo": "^0.18.2"
  },
  "xo": {
    "space": 2,
    "envs": [
      "browser"
    ],
    "globals": [
      "angular"
    ],
    "import/no-unassigned-import": {
      "allow": [
        "demo/index.js"
      ]
    }
  }
