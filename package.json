{
  "name": "angular-energy-label",
  "version": "0.0.1",
  "description": "An EU Energy Label component for AngularJS 1.5+",
  "main": "dist/angular-energy-label.js",
  "scripts": {
    "dev": "gulp serve:examples",
    "build": "gulp",
    "preversion": "npm run-script build && git add dist/."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/destegabry/angular-energy-label.git"
